{
  "type": "kafka",
  "ioConfig": {
    "type": "kafka",
    "consumerProperties": {
      "bootstrap.servers": "localhost:9092"
    },
    "topic": "test-topic",
    "inputFormat": {
      "type": "json"
    }
  },
  "dataSchema": {
    "dataSource": "my_weather_data",
    "timestampSpec": {
      "column": "__time",
      "format": "iso"
    },
    "dimensionsSpec": {
      "dimensions": [
        "area"
      ]
    },
    "metricsSpec": [
      {
        "type": "count",
        "name": "count"
      },
      {
        "type": "doubleSum",
        "name": "temp_sum",
        "fieldName": "temp"
      },
      {
        "type": "doubleSum",
        "name": "humidity_sum",
        "fieldName": "humidity"
      }
    ],
    "granularitySpec": {
      "type": "uniform",
      "segmentGranularity": "HOUR",
      "queryGranularity": "NONE",
      "rollup": true
    }
  },
  "tuningConfig": {
    "type": "kafka",
    "maxRowsPerSegment": 100,
    "maxRowsInMemory": 100,
    "partitions": 1,
    "replicas": 1
  }
}
